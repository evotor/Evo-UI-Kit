{
  "name": "evo-ui",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "ng": "ng",
    "gulp": "gulp",
    "build": "gulp",
    "build:icons": "gulp buildMonochromeIcons",
    "build:color-icons": "gulp buildColorIcons",
    "build:storybook": "gulp storybook",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:styles": "stylelint \"**/*.scss\"",
    "lint:styles:fix": "stylelint \"**/*.scss\" --syntax scss --fix",
    "lint:ts": "ng lint --quiet",
    "lint:ts:fix": "ng lint --quiet --fix",
    "publish-package": "npm install && npm run build && cd dist/evo-ui-kit && npm publish --registry http://npm.market.local",
    "release": "npm run build && ng test evo-ui-kit  --browsers Chrome --watch=false && cd projects/evo-ui-kit/ && npm run release && cd ../../",
    "start": "ng serve",
    "storybook": "start-storybook -p 9001",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "test:local": "ng test evo-ui-kit --browsers Chrome",
    "build-storybook": "build-storybook"
  },
  "resolutions": {
    "browserslist": "4.8.5",
    "caniuse-lite": "1.0.30001030",
    "chokidar": "~3",
    "@storybook/{app}/webpack": "^5"
  },
