{
  "name": "evo-ui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "gulp",
    "build:icons": "gulp buildMonochromeIcons",
    "build:color-icons": "gulp buildColorIcons",
    "build:storybook": "gulp storybook",
    "lint": "yarn lint:ts && yarn lint:styles",
    "lint:styles": "stylelint \"**/*.scss\"",
    "lint:styles:fix": "stylelint \"**/*.scss\" --syntax scss --fix",
    "lint:ts": "tslint -p ./tsconfig.json -c ./tslint.json",
    "lint:ts:fix": "yarn lint:ts --fix",
    "ng": "ng",
    "publish-package": "yarn install && yarn build && cd dist/evo-ui-kit && npm publish --registry http://npm.market.local",
    "release": "yarn build && ng test evo-ui-kit  --browsers Chrome --watch=false && cd projects/evo-ui-kit/ && npm run release && cd ../../",
    "start": "ng serve",
    "storybook": "start-storybook -p 9001 -s dist/evo-ui-kit,src/assets -c .storybook",
    "test:ci": "npm rebuild node-sass && npm run build && npm run build:icons && ng test evo-ui-kit --watch=false --browsers ChromeNoSandbox --codeCoverage=true",
    "test:local": "ng test evo-ui-kit --browsers Chrome"
  },
  "resolutions": {
    "browserslist": "4.8.5",
    "caniuse-lite": "1.0.30001030"
  },
