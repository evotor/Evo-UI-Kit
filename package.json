{
  "name": "evo-ui",
  "version": "0.0.0",
  "homepage": "https://evotor.github.io/Evo-UI-Kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/evotor/Evo-UI-Kit.git"
  },
  "private": false,
  "engines": {
    "node": ">=16.20.2",
    "yarn": "Please use npm instead of yarn to install dependencies"
  },
  "scripts": {
    "ng": "ng",
    "gulp": "gulp",
    "build": "gulp",
    "build:icons": "gulp buildMonochromeIcons",
    "build:color-icons": "gulp buildColorIcons",
    "build:storybook": "gulp storybook",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:styles": "stylelint \"**/*.scss\"",
    "lint:styles:fix": "stylelint \"**/*.scss\" --syntax scss --fix",
    "lint:ts": "ng lint --quiet",
    "lint:ts:fix": "ng lint --quiet --fix",
    "publish-package": "npm ci && npm run build && cd dist/evo-ui-kit && npm publish",
    "release": "npm run build && ng test evo-ui-kit  --browsers Chrome --watch=false && cd projects/evo-ui-kit/ && npm run release && cd ../../",
    "start": "ng serve",
    "storybook": "ng run evo-ui-kit:storybook",
    "test:ci": "npm run build && ng test evo-ui-kit --browsers=ChromeHeadless --watch=false --code-coverage",
    "test:local": "ng test evo-ui-kit --browsers Chrome"
  },
  "resolutions": {
    "@storybook/angular/webpack": "^5"
  },
  "dependencies": {
    "@angular/animations": "14.3.0",
    "@angular/cdk": "14.2.7",
